{
  "$schema": "http://json.schemastore.org/template",
  "author": "Zhang.Shaobo@outlook.com",
  "classifications": [
    "BlazorWASM"
  ],
  "identity": "ZhangShaobo.Templates.BlazorWASM",
  "name": "Blank Blazor WASM template",
  "shortName": "shaobo_wasm",
  "sourceName": "__PROJECT_NAME__",
  "defaultName": "HelloWASM",
  "symbols": {
    "HttpDevPort": {
      "type": "parameter",
      "datatype": "int",
      "description": "http listening port in Development",
      "defaultValue": "5018",
      "replaces": "__DEV_HTTP_PORT__"
    },
    "HttpsDevPort": {
      "type": "parameter",
      "datatype": "int",
      "description": "https listening port in Development",
      "defaultValue": "7284",
      "replaces": "__DEV_HTTPS_PORT__"
    }
  }
}